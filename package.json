{
  "name": "workshop-protractor",
  "version": "1.0.0",
  "description": "This is a workshop about Protractor",
  "main": "index.js",
  "scripts": {
    "lint": "tslint --project tsconfig.json protractor/**/*.ts test/**/*.ts src/**/*.ts",
    "clean": "rm -rf dist",
    "build": "run lint && npm run clean && tsc",
    "test:local": "npm run build && protractor dist/protractor/local.config.js",
    "test:headless": "npm run build && protractor dist/protractor/headless.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JCiroLo/protractor-workshop-2021.git"
  },
  "keywords": [
    "ui-tetsing",
    "dojo",
    "practice",
    "protractor"
  ],
  "author": "JuanCiroLo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JCiroLo/protractor-workshop-2021/issues"
  },
  "homepage": "https://github.com/JCiroLo/protractor-workshop-2021#readme",
  "devDependencies": {
    "@types/jasminewd2": "^2.0.10",
    "@types/node": "^16.11.6",
    "protractor": "^7.0.0",
    "tslint": "^6.1.3",
    "tslint-config-airbnb": "^5.11.2",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "jasmine-spec-reporter": "^7.0.0"
  }
